      @import url("https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700&display=swap");

      * {
        box-sizing: border-box;
        padding: 0;
        margin: 0;
      }

      body {
        background: #131325;
        font-family: "Roboto", sans-serif;
        height: 100vh;
      }

      .hero-container {
        margin: auto;
        display: flex;
        justify-content: center;
        flex-wrap: wrap;
        margin-top: 5rem;
      }

      .main-container {
        width: 270px;
        height: 540px;
        position: relative;
        margin: 0 20px;
      }

      .poster-container {
        width: 230px;
        height: 320px;
        position: absolute;
        top: 0;
        left: 20px;
        z-index: 9999;
      }

      .poster {
        width: 100%;
        height: 100%;
        box-shadow: 0 5px 20px 3px rgba(0, 0, 0, 0.6);
      }

      .ticket-container {
        background: #fff;
        width: 270px;
        height: 450px;
        display: flex;
        flex-direction: column;
        align-items: center;
        border-radius: 5px;
        position: absolute;
        top: 70px;
        box-shadow: 0 5px 20px 3px rgba(0, 0, 0, 0.6);
        opacity: 0;
      }

      .main-container:hover .ticket-container {
        opacity: 1;
        animation: bounceIn 0.6s linear;
      }

