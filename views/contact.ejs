  <body>
    <%- include('./partials/navBar.ejs') %>
      <div class="container-contact">
        <div class="forms-container">
          <div class="signin-signup">
            <form action="#" class="sign-in-form">
              <h2 class="title-contact">CONTACT US</h2>
              <div class="input-field">
                <i class="fas fa-user"></i>
                <input type="text" placeholder="Name" name="name" required />
              </div>
              <div class="input-field">
                <i class="fas fa-envelope"></i>
                <input type="email" placeholder="Email" name="email" required />
              </div>
              <div class="input-field">
                <i class="fas fa-comment"></i>
                <input
                  type="text"
                  placeholder="Your Message"
                  name="message"
                  required
                />
              </div>
              <input
                type="submit"
                value="send message"
                class="btn-contact solid"
              />
              <p class="social-text">Or Contact us in social platforms</p>
              <div class="social-media">
                <a href="https://www.facebook.com/wissenaireiitbbs/" class="social-icon">
                  <i class="fab fa-facebook-f"></i>
                </a>
                <a href="https://mobile.twitter.com/wissiitbbs" class="social-icon">
                  <i class="fab fa-twitter"></i>
                </a>
                <a href="https://www.instagram.com/wissenaire.iitbbs/" class="social-icon">
                  <i class="fab fa-instagram"></i>
                </a>
                <a href="https://www.google.com/url?sa=t&rct=j&q=&esrc=s&source=web&cd=&cad=rja&uact=8&ved=2ahUKEwiNstDD0eL8AhUtTGwGHS5mCoAQFnoECA8QAQ&url=https%3A%2F%2Fin.linkedin.com%2Fcompany%2Fwissenaireiitbbs&usg=AOvVaw23qvfAF3jknfp1yA08giRU" class="social-icon">
                  <i class="fab fa-linkedin-in"></i>
                </a>
              </div>
            </form>
            <script>
              $("form").submit(function (e) {
                e.preventDefault();
                var form = $(this);
                console.log("ok");
                $.ajax({
                  type: "POST",
                  url: "/contact",
                  data: form.serialize(),
                  success: function (result) {
                    if (result) {
                      new swal({
                        title: "SUCCESS",
                        text: "Your message has been received successfully. We will get back soon.",
                        icon: "success",
                      }).then((flag) => {
                        if (flag) {
                          window.location.href = "/reachus";
                        }
                      });
                    } else {
                      new swal({
                        title: "Error",
                        text: "Please try again",
                        icon: "error",
                      });
                    }
                  },
                });
              });
            </script>
          </div>
        </div>

        <div class="panels-container">
          <div class="panel left-panel">
            <div class="content">
              <h3>Have questions?</h3>
              <p>We are just a message away from you!</p>
              <!-- <button class="btn-contact transparent" id="sign-up-btn">Contact</button> -->
            </div>
