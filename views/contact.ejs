} */
      .about-me .content h3 {
        font-weight: 700;
        font-size: 26px;
        color: #ff7e00;
      }
      .about-me .content ul {
        list-style: none;
        padding: 0;
      }
      .about-me .content ul li {
        padding-bottom: 10px;
      }
      .about-me .content ul i {
        font-size: 20px;
        padding-right: 2px;
        color: #ff7e00;
      }
      .about-me .content p:last-child {
        margin-bottom: 0;
      }
      @keyframes rotate {
        from {
          transform: rotate(0deg);
        }
        to {
          transform: rotate(359deg);
        }
      }
      @keyframes cursorAnim {
        from {
          transform: scale(1);
        }
        to {
          transform: scale(0.7);
        }
      }
      @keyframes cursorClick {
        0% {
          transform: scale(1);
        }
        50% {
          transform: scale(2);
        }
        100% {
          transform: scale(1);
          opacity: 0;
        }
      }
      .expand {
        animation: cursorClick 0.5s forwards;
        border: 1px solid blue;
      }
      .link-grow {
        transform: scale(2);
        background: #1a28cf;
      }
      @media only screen and (max-width: 800px) {
        .cur {
          display: none;
        }
        body {
          cursor: default;
        }
      }
      body {
        width: 100vw;
        height: auto;
        background-color: rgb(0, 0, 0);
      }
      a:hover {
        text-decoration: none !important;
      }
      body {
        overflow-y: scroll;
      }
      ::-webkit-scrollbar {
        width: 0px;
        background: transparent;
      }
      .contact {
        color: #fff;
        margin-bottom: 50px;
      }
      .contactSpan {
        color: #000;
        display: inline-block;
        vertical-align: middle;
        line-height: normal;
      }
